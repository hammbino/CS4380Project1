A1    .INT  1
A2    .INT  2
A3    .INT  3
J     .BYT  'J'
E     .BYT  'E'
F     .BYT  'F'
SP    .BYT  'space'
NL    .BYT  '\n'
START LDB R3 J
     TRP 3
     LDB R3 E
     TRP 3
     LDB R3 F
     TRP 3
TRAP  TRP 3
     LDR R1 A1
     LDR R2 A2
     ADD R1 R2
     MOV R3 R1
     TRP 1 ;Print 3
     LDB R3 NL
     TRP 3 ;Print space
     TRP 3 ;Print space
     LDR R2 A3
     ADD R1 R2
     MOV R3 R1
     TRP 1   ;Print 6
SSUB  LDB R3 NL
     TRP 3   ;Print space
     TRP 3   ;Print space